{
  "name": "maksimovic/country-enums",
  "description": "All (or at least most) countries and their regions formatted as PHP enums",
  "type": "library",
  "license": "MIT",
  "autoload": {
	"psr-4": {
	  "CountryEnums\\": "src/"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "Tests\\": "tests/"
	}
  },
  "authors": [
	{
	  "name": "Bradie Tilley",
	  "homepage": "https://github.com/bradietilley"
	},
	{
	  "name": "Oliver Maksimovic",
	  "homepage": "https://github.com/maksimovic"
	}
  ],
  "require": {
	"php": "^8.1"
  },
  "require-dev": {
	"phpunit/phpunit": "^10.5 || ^12.5"
  },
  "suggest": {
	"illuminate/collections": "^10",
	"illuminate/validation": "^10"
  },
  "scripts": {
	"test": "./vendor/bin/phpunit",
	"test-coverage": "php -d xdebug.mode=coverage ./vendor/bin/phpunit --coverage-html=tests/coverage -dmemory_limit=256M"
  }
}
